<style>
	html {
				background-image: url('/images/butterfly-wallpaper-bnw.jpg');
				background-repeat: no-repeat;
			}
</style>
	<h1>Index of butterflies</h1>
		<ul>
  			<% @butterflies.each do |butterfly| %>
    		<li>
     			<a href="/butterflies/<%= butterfly['id'] %>">
        		<img class="thumb" src="<%= butterfly["photo"] %>">
        		<%= butterfly["name"] %>
      			</a>
    		</li>
  			<% end %>
		</ul>